syntax = "v1"

type GetTotalDownloadsRequest {
	org       string `path:"org"`
	repo      string `path:"repo"`
	container string `path:"container"`
}

type GetTotalDownloadsResponse {
	message string `json:"message"`
}

@server (
	prefix: /api/v1
	group:  shields/github
)
service goServerlessVercel {
	@handler GetTotalDownloadsHandler
	get /shields/github/:org/:repo/pkgs/container/:container/downloads (GetTotalDownloadsRequest) returns (GetTotalDownloadsResponse)
}

